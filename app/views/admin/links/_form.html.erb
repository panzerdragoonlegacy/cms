<%= form_for [:admin, @link] do |f| %>
  <%= render 'admin/shared/form/error_messages', target: @link %>
  <div class="form-group form-check">
    <%= f.check_box :partner_site, class: 'form-check-input' %>
    <%= f.label :partner_site, 'Partner Site?', class: 'form-check-label' %>
  </div>
  <%= render partial: 'admin/shared/form/name', locals: { f: f } %>
  <div class="form-group">
    <%= f.label :url, 'URL*' %>
    <%= f.text_field :url, class: 'form-control' %>
  </div>
  <%= render partial: 'admin/shared/form/save_buttons', locals: { f: f } %>
<% end %>
