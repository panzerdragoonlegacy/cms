<div class="form-group">
  <%= f.label :music_track_picture, 'Music Track Picture' %>
  <% if music_track.music_track_picture? %>
    <% if is_jpeg music_track.music_track_picture.url %>
      <p><%= show_picture music_track, 'music_track_picture', :thumbnail %></p>
    <% end %>
    <p>File has already been uploaded. Replace file?</p>
  <% end %>
  <%= f.file_field :music_track_picture, class: 'form-control' %>
  <%= render 'admin/shared/form/image_type_warning' %>
</div>
