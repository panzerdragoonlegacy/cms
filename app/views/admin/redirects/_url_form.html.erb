    <%= f.label :old_url_or_new_url_cont, 'Old or New URL:', class: 'mr-1' %>
    <%= f.search_field :old_url_or_new_url_cont, class: 'mr-2' %>
    <%= render partial: 'admin/shared/search/search_button', locals: { f: f } %>
  </div>
<% end %>
