<%= form_for [:admin, @saga] do |f| %>
  <%= render 'admin/shared/form/error_messages', target: @saga %>
  <%= render partial: 'page', locals: { f: f, pages: @pages } %>
  <%= render partial: 'admin/shared/form/sequence_number', locals: { f: f } %>
  <%= render partial: 'admin/shared/form/name', locals: { f: f } %>
  <%= render partial: 'admin/shared/form/save_buttons', locals: { f: f } %>
<% end %>
